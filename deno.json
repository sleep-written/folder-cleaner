{
    "imports": {
        "@utils/": "./src/utils/",

        "@inquirer/prompts": "npm:@inquirer/prompts@^7.8.3",
        "chalk": "npm:chalk@^5.6.0",
        "decimal.js": "npm:decimal.js@^10.6.0"
    },
    "tasks": {
        "install": "deno install --allow-scripts=npm:@swc/core",
        "execute": "deno run --allow-read --allow-write --allow-env ./src/index.ts",
        "compile": "deno compile --allow-read --allow-write --allow-env -o folder-cleaner ./src/index.ts"
    }
}